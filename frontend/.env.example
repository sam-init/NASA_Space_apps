# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=30000

# Environment
REACT_APP_ENV=development

# Features flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false

# External Services (if needed)
REACT_APP_SENTRY_DSN=
REACT_APP_GOOGLE_ANALYTICS_ID=

# App Configuration
REACT_APP_NAME="ExoPlanet AI"
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION="AI-powered exoplanet detection using NASA datasets"

# File Upload Limits
REACT_APP_MAX_FILE_SIZE=104857600
REACT_APP_ALLOWED_FILE_TYPES=.csv,.fits,.dat,.txt

# NASA API (if using official NASA APIs)
REACT_APP_NASA_API_KEY=

# Development
GENERATE_SOURCEMAP=true
FAST_REFRESH=true
